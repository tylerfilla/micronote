<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gmail.tylerfilla.android.notes"
    android:versionCode="0"
    android:versionName="@string/app_version_name" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Notes.Theme" >
        <activity android:name="com.gmail.tylerfilla.android.notes.activity.ActivityList" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.gmail.tylerfilla.android.notes.activity.ActivityEdit" />
        <activity
            android:name="com.gmail.tylerfilla.android.notes.activity.ActivitySettings"
            android:label="Settings" />
    </application>

</manifest>